{
  "manifest_version": 2,
  "key": "",

  "name": "Pass for Chrome",
  "description": "This extension allows usage of pass in chrome",
  "version": "1.0",

  "background" : {
    "scripts" : [
      "background.js"
    ]
  },

  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": [
      "content.js"
    ]
  }],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "nativeMessaging",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "clipboardWrite"
  ]
}
